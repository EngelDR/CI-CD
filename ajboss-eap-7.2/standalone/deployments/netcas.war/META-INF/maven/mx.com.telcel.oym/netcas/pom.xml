			</excludes>
		</resource>
		<resource>
			<directory>src/main/webapp</directory>
			<excludes>
				<exclude>**/*.java</exclude>
			</excludes>
		</resource>
		<resource>
			<directory>${basedir}/src/main/webapp/WEB-INF/lib</directory>
			<targetPath>WEB-INF/lib</targetPath>
			<includes>
